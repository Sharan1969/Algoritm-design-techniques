#include <bits/stdc++.h>
using namespace std;

int main() {
	string m1,m2;
	getline(cin,m1);
	getline(cin,m2);
	int n=m1.length();
	int m=m2.length();
	vector<vector<int>>dp(n+1,vector<int>(m+1,0));
	for(int i=0;i<n;i++){
	    for(int j=0;j<m;j++){
	        dp[i][j]=0;
	    }
	}
	for(int i=1;i<=n;i++){
	    for(int j=1;j<=m;j++){
	        if(m1[i-1]==m2[j-1]){
	            dp[i][j]=1+dp[i-1][j-1];
	        }else{
	            dp[i][j]=max(dp[i-1][j],dp[i][j-1]);
	        }
	    }
	}
	int i=n,j=m;
	string lcs="";
	while(i>0 && j>0){
	    if(m1[i-1]==m2[j-1]){
	        lcs=m1[i-1]+lcs;
	        i--;
	        j--;
	    }else if(dp[i-1][j]>dp[i][j-1]){
	        i--;
	    }else{
	        j--;
	    }
	}
	cout<<lcs<<endl;
	cout<<"The length of the Longest Common Subsequence is: "<<dp[n][m];
}

// Input 1 :
// This is test
// is test This

// Output 1 :
// is test
// The length of the Longest Common Subsequence is: 7
