#include <bits/stdc++.h>
using namespace std;

bool subset(vector<int>&arr,int target,int n,vector<int>&comb){
    int count=0;
    int totalsub=1<<n;
    
    for(int i=0;i<totalsub;i++){
        int sum=0;
        vector<int>temp;
        for(int j=0;j<n;j++){
            if(i & (1<<j)){
            sum+=arr[j];
            temp.push_back(arr[j]);
            }
        }
        if(sum==target){
            comb=temp;
            return true;
            break;
        }
    }
    return false;
}

int main() {
    int n;
    cin>>n;
    vector<int>arr(n);
     vector<int>combination;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int target;
    cin>>target;
    cout<<(target+10)*3<<endl;
    if(subset(arr,target,n,combination)){
    for(int i=0;i<combination.size();i++){
        cout<<combination[i]<<" ";
    }
    }
}

// Input 1 :
// 5
// 2 4 6 10 3
// 13

// Output 1 :
// 69
// 4 6 3
