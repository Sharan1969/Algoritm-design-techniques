#include<bits/stdc++.h>
using namespace std;
int main(){
    string m1,m2;
    cin>>m1>>m2;
    int ins,del,upt;
    cin>>ins>>del>>upt;
    int n=m1.length();
    int m=m2.length();
     vector<vector<int>>dp(n+1,vector<int>(m+1,0));
    for(int i=0;i<n;i++){
        dp[i][0]=i*del;
        for(int j=0;j<m;j++){
            dp[0][j]=j*ins;
        }
    }
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            if(m1[i-1]==m2[j-1]){
                dp[i][j]=dp[i-1][j-1];
            }else{ 
                int d=dp[i-1][j]+del;
                int insrt=dp[i][j-1]+ins;
                int up=dp[i-1][j-1]+upt;
                d=min(d,insrt);
                dp[i][j]=min(d,up);
            }
        }
    }
    cout<<dp[n][m];
}


// Input 1 :
// book
// back
// 4
// 3
// 5

// Output 1 :
// 10
