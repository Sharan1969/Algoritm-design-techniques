#include <bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int s;
	cin>>s;
	vector<int>dp(n+1,0);
	dp[0]=1;
	for(int x:arr){
	    for(int i=x;i<=s;i++){
	        dp[i]+=dp[i-x];
	    }
	}
	cout<<dp[s];
}

// Input 1 :
// 3
// 1 2 3
// 3

// Output 1 :
// 3
